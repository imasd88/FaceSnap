<?xml version="1.0" encoding="UTF-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0000"
    tools:context="facesnap.emazdoor.com.facesnap.MainActivity">


    <android.support.constraint.Guideline
        android:id="@+id/verticalGuide_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="0dp" />

    <android.support.constraint.Guideline
        android:id="@+id/verticaleGuide_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.93" />

    <android.support.constraint.Guideline
        android:id="@+id/horizontalGuide_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.12" />

    <android.support.constraint.Guideline
        android:id="@+id/horizontalGuide_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.31" />


    <android.support.constraint.Guideline
        android:id="@+id/horizontalGuide_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.6" />

    <Button
        android:id="@+id/captureButton"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="170dp"
        android:layout_height="150dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="8dp"
        android:background="@drawable/camera_img"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/verticalGuide_1"
        app:layout_constraintRight_toLeftOf="@+id/verticaleGuide_2"
        app:layout_constraintTop_toBottomOf="@+id/logoImage"
        tools:background="@android:drawable/presence_invisible"
        tools:backgroundTint="@android:color/white"
        tools:ignore="UnusedAttribute" />


    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:shadowColor="#31caf0"
        android:shadowDx="1.5"
        android:shadowDy="1.5"
        android:shadowRadius="2.5"
        android:text=" Robotics "
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Widget.TextView"
        android:textSize="28dp"
        android:visibility="visible"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="@+id/verticalGuide_1"
        app:layout_constraintRight_toLeftOf="@+id/verticaleGuide_2"
        app:layout_constraintTop_toBottomOf="@+id/captureButton"
        tools:ignore="HardcodedText,MissingConstraints,SpUsage" />

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:shadowColor="#31caf0"
        android:shadowDx="1.5"
        android:shadowDy="1.5"
        android:shadowRadius="2.5"
        android:text="for Work and Play "
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Widget.TextView"
        android:textSize="25dp"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/verticalGuide_1"
        app:layout_constraintRight_toLeftOf="@+id/verticaleGuide_2"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="HardcodedText,MissingConstraints,SpUsage" />

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:shadowColor="#31caf0"
        android:shadowDx="1.5"
        android:shadowDy="1.5"
        android:shadowRadius="2.5"
        android:text=" from Anywhere"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Widget.TextView"
        android:textSize="25dp"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/verticalGuide_1"
        app:layout_constraintRight_toLeftOf="@+id/verticaleGuide_2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="HardcodedText,MissingConstraints,SpUsage" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="532dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="532dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.84162897"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="302dp" />

    <ImageButton
        android:id="@+id/settingsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="15dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/blank_circle_bg"
        android:contentDescription=""
        app:layout_constraintBottom_toTopOf="@+id/horizontalGuide_2"
        app:layout_constraintLeft_toLeftOf="@+id/verticaleGuide_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@mipmap/settings"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/logoImage"
        android:layout_width="468dp"
        android:layout_height="384dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/transparent_bg"
        android:elevation="5dp"
        android:foreground="@drawable/rounded"
        android:padding="3dp"
        android:paddingBottom="3dp"
        android:paddingEnd="3dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:paddingStart="3dp"
        android:paddingTop="3dp"
        android:shape="ring"

        android:useLevel="false"
        app:layout_constraintBottom_toTopOf="@+id/horizontalGuide_3"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintLeft_toLeftOf="@+id/verticalGuide_1"
        app:layout_constraintRight_toLeftOf="@+id/verticaleGuide_2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.034"
        app:srcCompat="@drawable/pixelf"
        tools:ignore="ContentDescription" />


</android.support.constraint.ConstraintLayout>

